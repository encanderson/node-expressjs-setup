generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URI")
}

model User {
  id          String     @id @default(uuid())
  active      Boolean
  name        String
  email       String     @unique
  password    String
  code        Int?
  createdAt   DateTime
  updatedAt   DateTime

  @@map("users")
}